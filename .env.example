# Contentstack Configuration (Required)
REACT_APP_CONTENTSTACK_API_KEY=your_api_key_here
REACT_APP_CONTENTSTACK_DELIVERY_TOKEN=your_delivery_token_here
REACT_APP_CONTENTSTACK_ENVIRONMENT=your_environment_here

# Contentstack Region (Optional - defaults to US)
# Options: US, EU, AZURE_NA, AZURE_EU, GCP_NA
REACT_APP_CONTENTSTACK_REGION=US

# Contentstack Hosts (Optional - will use defaults if not provided)
REACT_APP_CONTENTSTACK_APP_HOST=app.contentstack.com
REACT_APP_CONTENTSTACK_API_HOST=api.contentstack.io

# Live Preview Configuration (Required for Live Preview)
REACT_APP_CONTENTSTACK_LIVE_PREVIEW=true
REACT_APP_CONTENTSTACK_PREVIEW_TOKEN=your_preview_token_here

# Live Preview Features (Optional)
REACT_APP_CONTENTSTACK_LIVE_EDIT_TAGS=true

# Default Mobile Phone UID for fallback (Optional)
REACT_APP_MOBILE_PHONE_UID=bltffc3e218b0c94c4a

# Security & Performance (Optional)
GENERATE_SOURCEMAP=false
REACT_APP_ENABLE_ERROR_REPORTING=true