# Contentstack Configuration
REACT_APP_CONTENTSTACK_API_KEY=your_contentstack_api_key_here
REACT_APP_CONTENTSTACK_DELIVERY_TOKEN=your_delivery_token_here
REACT_APP_CONTENTSTACK_ENVIRONMENT=production
REACT_APP_CONTENTSTACK_REGION=us
REACT_APP_CONTENTSTACK_LIVE_PREVIEW=false

# Optional: Preview Configuration (for content preview)
REACT_APP_CONTENTSTACK_PREVIEW_HOST=preview-rest.contentstack.io
REACT_APP_CONTENTSTACK_PREVIEW_TOKEN=your_preview_token_here

# Optional: Live Preview Configuration (for real-time editing)
REACT_APP_CONTENTSTACK_LIVE_PREVIEW_MANAGEMENT_TOKEN=your_management_token_here
REACT_APP_CONTENTSTACK_LIVE_PREVIEW_HOST=api.contentstack.io

# Contentstack Personalize Configuration
# Get these from your Contentstack Personalize project
REACT_APP_CONTENTSTACK_PERSONALIZE_PROJECT_UID=your_personalize_project_uid_here
REACT_APP_CONTENTSTACK_PERSONALIZE_EDGE_API_URL=https://personalize-api.contentstack.com

# Default Content Configuration
REACT_APP_MOBILE_PHONE_UID=bltffc3e218b0c94c4a

# Optional: Analytics and Debugging
REACT_APP_ENABLE_PERSONALIZATION_LOGGING=true

# Regional API Endpoints (uncomment based on your region)
# US (default)
# REACT_APP_CONTENTSTACK_PERSONALIZE_EDGE_API_URL=https://personalize-api.contentstack.com

# EU
# REACT_APP_CONTENTSTACK_PERSONALIZE_EDGE_API_URL=https://eu-personalize-api.contentstack.com

# Azure NA
# REACT_APP_CONTENTSTACK_PERSONALIZE_EDGE_API_URL=https://azure-na-personalize-api.contentstack.com

# Azure EU
# REACT_APP_CONTENTSTACK_PERSONALIZE_EDGE_API_URL=https://azure-eu-personalize-api.contentstack.com

# GCP NA
# REACT_APP_CONTENTSTACK_PERSONALIZE_EDGE_API_URL=https://gcp-na-personalize-api.contentstack.com